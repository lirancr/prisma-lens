{
  "name": "prisma-lens",
  "version": "0.1.1",
  "description": "Alternative migration client for prisma ORM",
  "main": "src/index.js",
  "scripts": {
    "build": "npx -p typescript tsc",
    "pretest": "cd ./test/test-project && npm install",
    "test": "npx jest --runInBand",
    "test:ci": "TEST_DATABASE_URL=postgres://postgres@127.0.0.1:5432/testdb TEST_SHADOW_DATABASE_URL=postgres://postgres@127.0.0.1:5432/prisma_shadow TEST_SHADOW_DATABASE_NAME=plens_shadow npx jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lirancr/prisma-lens/"
  },
  "bin": {
    "plens": "./bin/client.js"
  },
  "engines": {
    "node": "14.15.x"
  },
  "author": "lirancr",
  "license": "ISC",
  "dependencies": {
    "execa": "^5.1.1",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@types/execa": "^2.0.0",
    "@types/jest": "^27.0.2",
    "@types/minimist": "^1.2.2",
    "@types/node": "^16.9.6",
    "jest": "^27.2.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2",
    "@prisma/client": "^3.1.1",
    "prisma": "^3.1.1"
  },
  "peerDependencies": {
    "@prisma/client": "^3.x",
    "prisma": "^3.x"
  }
}
